public class Main
{
	public static void main(String[] args) {
		int n = 1231;
		int rem;
		int rev = 0;
		int temp = n;
		while (n != 0){
		    rem = n%10;
		    rev = rev * 10 + rem;
		    n = n/10;
		}
		if(temp == rev){
		    System.out.println("Number is palindrome");
		}
		else{
		    System.out.println("Number is not a palindrome");
		}
	}
}
